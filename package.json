{
  "name": "Twidget2",
  "version": "1.0.0",
  "description": "Companion App for khaata.in",
  "main": "bootstrap.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build-osx": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "build-win32": "electron-packager . khaata-widget --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=KH --version-string.FileDescription=KH --version-string.ProductName='Khaata Widget'",
    "pack": "build --dir"
  },
  "keywords": [],
  "author": "Mitalee Mulpuru",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "npm": "^6.4.1"
  }
}
